{
    "name": "fresh/centrifugo-bundle",
    "description": "Provides communication with web-socket server Centrifugo in Symfony applications.",
    "keywords": ["bundle", "symfony", "centrifugo", "web socket", "web-socket", "socket", "real time"],
    "homepage": "https://github.com/fre5h/FreshCentrifugoBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/fre5h/FreshCentrifugoBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/FreshCentrifugoBundle/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/doctrine-bridge": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/http-kernel": "^4.0",
        "symfony/validator": "^4.0",
        "twig/twig": "~2.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-phpunit": "^0.10",
        "phpunit/phpunit": "^7.5",
        "escapestudios/symfony2-coding-standard": "^3.4",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\CentrifugoBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\CentrifugoBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
